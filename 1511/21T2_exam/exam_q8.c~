#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define MAX_WORD_LENGTH 100

int main(void) {
    int places_before = 0;
    char *word1[MAX_WORD_LENGTH];
    char *word2[MAX_WORD_LENGTH];
    char *storage[MAX_WORD_LENGTH] = {"example"};
    int i = 0;
    storage[i] = getchar();
    while (*storage[i] != '\n') {
        i++;
        storage[i] = getchar();
    }
    printf("%s", storage);
    
    
    return 0;
}
